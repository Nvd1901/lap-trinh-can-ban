#include <stdio.h>

int main()
{
    printf("moi ban nhap so ");
    int n;
    scanf("%d", &n);
    if(n<=2){
        if(n==0){
            printf("0");
        }
        else{
            printf("2");
        }

    }
    else{
        int a=1,b=1;
    int sum;
    for(int i=2;i<=n-1;i++){
        sum=a+b;
        a=b;
        b=sum;
        
    }
    printf("Fibonacci(%d) = %d", n,sum);
    return 0;
    }
    
}
