#include <stdio.h>
long long luythua(int a, int b){
    long x=1;
    for (int i=1;i<=b;i++){
        x*=a;
    }
    return x;
    
}
int main()
{ int a;
int b;
long x;
printf("Moi ban nhap co so ");
scanf("%d",&a);
printf("Moi ban nhap so mu ");
scanf("%d",&b);
x=luythua(a,b);
printf("luy thua la %ld",x);
    return 0;
}
