#include <stdio.h>
void chuyendoi(int *a, int *b){
    int x = *a;
    *a= *b;
    *b= x;
}
int main()
{
    int a; int b;
    printf("Moi ban nhap so a ");
    scanf("%d", &a);
    printf("Moi ban nhap so b ");
    scanf("%d", &b);
    chuyendoi(&a,&b);
    printf("Hai so sau hoan doi la a=%d va b=%d",a,b);
    

    return 0;
}
