#include <stdio.h>
void chuyendoi(int *a, int *b){
    int x = *a;
    *a= *b;
    *b= x;
}
int main()
{
    int a[10];
    for(int i=0;i<10;i++){
        printf("Moi ban nhap so thu %d ",i+1);
        scanf("%d",&a[i]);
    }
    int *ptr = a;
    printf("Day so la ");
    for(int i=0;i<10;i++){
        printf("%d ",*(ptr+i));
    }
    return 0;
}
