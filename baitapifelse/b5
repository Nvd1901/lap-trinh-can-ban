
 #include <stdio.h>
#include <math.h>

int main() {
    float a;
    float b;
    float c;
    scanf("%f",&a);
    scanf("%f",&b);
    scanf("%f",&c);
    if (a==0){
        printf("pt co 1 nghiem %f", -c/b);
    }
    else {
        float delta= b*b - 2*a*c;
        if (delta<0){
        printf("pt vo nghiem");
    }
        else if (delta==0){
        printf("pt co mot nghiem %f",-b/(2*a));
        
    }
        else{
        float x1= (-b + sqrt(delta))/(2*a);
        float x2= (-b - sqrt(delta))/(2*a);
        printf("pt co hai nghiem %f,%f", x1,x2);
        
    }
        
    }
    
}
